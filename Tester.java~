public class Tester
{
  public static void main(String[]args)
  {
    //Classname variable = new Classname()
    Student s1 = new Student();//object1/instant1
    Student s2 = new Student();
    s1.name= "mohin";
    s1.id= "11";
    s2.name= "mahmud";
    s2.id= "12";
    s1.m1();
   System.out.println(s1.name+" "+s1.id);
   System.out.println(s2.name+" "+s2.id);
   s1=s2;
   s1.name="tasir";
   System.out.println(s1.name+" "+s1.id);
   System.out.println(s2.name+" "+s2.id);
  }
}