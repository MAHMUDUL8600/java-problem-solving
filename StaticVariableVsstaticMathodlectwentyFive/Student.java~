public class Student
{
  public String name;
  public int id;
  public String uniName = "Bracu";
  
  public Student (String n, int i){
    name = n;
    id = i;
  }
  
  public void showDetail(){
    System.out.println("Name: "+name);
    System.out.println("Name: "+id);
    System.out.println("University: "+uniName);
  }
}